{
    "compilerOptions": {
      "module": "commonjs",
      "baseUrl": "./",
      "outDir": "build",
      "target": "es2015",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "lib": ["esnext"],
      "allowJs": true,
      "forceConsistentCasingInFileNames": true,
    },
    "include": [
      "**.ts"
    ],
    "exclude": [
      "./src"
    ]
  }